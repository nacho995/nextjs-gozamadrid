{
  "version": 1,
  "include": [
    "/_next/data/*",
    "/api/*"
  ],
  "exclude": [],
  "routes": [
    {
      "pattern": "/_next/data/[^/]+/(.+).json",
      "script": "api/json-data.js"
    },
    {
      "pattern": "/api/proxy",
      "script": "api/proxy.js"
    },
    {
      "pattern": "/api/test",
      "script": "api/test.js"
    }
  ]
} 