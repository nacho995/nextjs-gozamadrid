# Variables de entorno para WooCommerce
# IMPORTANTE: Estas credenciales deben configurarse en Vercel y en desarrollo local

# Credenciales de WooCommerce (REQUERIDAS)
WC_CONSUMER_KEY=tu_consumer_key_aqui
WC_CONSUMER_SECRET=tu_consumer_secret_aqui

# URL de la API de WooCommerce
WC_API_URL=https://wordpress.realestategozamadrid.com/wp-json/wc/v3

# Credenciales públicas de WooCommerce (para frontend - opcional)
NEXT_PUBLIC_WOO_COMMERCE_KEY=tu_consumer_key_aqui
NEXT_PUBLIC_WOO_COMMERCE_SECRET=tu_consumer_secret_aqui
NEXT_PUBLIC_WC_API_URL=https://wordpress.realestategozamadrid.com/wp-json/wc/v3

# URL del sitio
NEXT_PUBLIC_SITE_URL=https://realestategozamadrid.com

# MongoDB (si se usa)
MONGODB_URI=tu_mongodb_uri_aqui

# Configuración de Vercel
VERCEL_URL=
VERCEL_ENV=development 