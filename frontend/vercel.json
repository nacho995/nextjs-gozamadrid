{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "out",
  "framework": "nextjs",
  "github": {
    "silent": true
  },
  "cleanUrls": true,
  "trailingSlash": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 