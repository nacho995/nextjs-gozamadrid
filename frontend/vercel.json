{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "regions": ["fra1"],
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ],
  "github": {
    "silent": true
  },
  "ignoreCommand": "find . -name '.next' -type d -prune -o -name '.vercel' -type d -prune -o -path './node_modules' -prune -o -print"
} 