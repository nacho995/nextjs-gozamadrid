{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && touch dist/index.html && cp dist/vender-comprar.html dist/index.html",
    "start": "next start -p ${PORT:-8081}",
    "lint": "next lint",
    "deploy:aws": "npm run build && eb deploy",
    "eb:init": "eb init",
    "eb:create": "eb create",
    "eb:deploy": "eb deploy",
    "export": "NEXT_EXPORT=true next build && node scripts/copy-to-cloudflare.js",
    "generate-sitemap": "next-sitemap",
    "analyze": "ANALYZE=true next build",
    "deploy:cloudflare": "wrangler pages deploy out --project-name=gozamadrid-frontend-new --commit-dirty=true"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@relume_io/relume-tailwind": "^1.3.0",
    "@relume_io/relume-ui": "^1.3.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.14",
    "axios": "^1.8.3",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "formidable": "^3.5.2",
    "framer-motion": "^10.12.16",
    "js-cookie": "^3.0.5",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.0",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^2.7.0",
    "parallax-js": "^3.1.0",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.5.2",
    "react-spring": "9.6.1",
    "rellax": "^1.12.1",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.3.3",
    "tailwindcss-textshadow": "^2.1.3",
    "toast": "^0.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@eslint/eslintrc": "^2.1.2",
    "@swc/helpers": "^0.5.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.42.0",
    "eslint-config-next": "14.0.0",
    "glob": "^11.0.1",
    "react-refresh": "^0.14.0"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@react-three/fiber": "8.15.19"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
