{
  "name": "gozamadrid-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "pwd && CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=1000 next dev",
    "dev-no-refresh": "FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true next dev",
    "dev-stable": "FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true NEXT_DISABLE_HMR=true next dev",
    "dev-sin-fast-refresh": "FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true NEXT_DISABLE_HMR=true NODE_ENV=development next dev",
    "dev-production": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "deploy:aws": "npm run build && eb deploy",
    "eb:init": "eb init",
    "eb:create": "eb create",
    "eb:deploy": "eb deploy",
    "generate-sitemap": "next-sitemap",
    "analyze": "ANALYZE=true next build",
    "deploy:cloudflare": "wrangler pages deploy out --project-name=gozamadrid-frontend-new --commit-dirty=true",
    "check-woocommerce": "node ../scripts/check-woocommerce.js",
    "check-vercel-env": "node ../scripts/check-vercel-env.js",
    "check-backend": "node ../scripts/check-backend-connection.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.3.1",
    "@heroicons/react": "^2.1.5",
    "@react-google-maps/api": "^2.20.6",
    "@relume_io/relume-tailwind": "^1.3.0",
    "@relume_io/relume-ui": "^1.3.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/js-cookie": "^3.0.6",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/kv": "^3.0.0",
    "autoprefixer": "10.4.21",
    "axios": "^1.8.4",
    "caniuse-lite": "1.0.30001720",
    "critters": "^0.0.24",
    "date-fns": "^4.1.0",
    "fast-glob": "3.3.3",
    "fastq": "1.19.1",
    "formidable": "^3.5.2",
    "framer-motion": "^11.15.0",
    "js-cookie": "^3.0.5",
    "mongodb": "6.17.0",
    "mongoose": "^8.15.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.3.3",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.10.1",
    "normalize-path": "3.0.0",
    "openai": "^4.103.0",
    "parallax-js": "^3.1.0",
    "postcss": "^8.5.4",
    "postcss-selector-parser": "7.1.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.13.1",
    "react-spring": "^9.6.1",
    "react-toastify": "^11.0.5",
    "rellax": "^1.12.1",
    "sharp": "^0.33.5",
    "sonner": "^2.0.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-textshadow": "^2.1.3",
    "toast": "^0.5.4",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@swc/helpers": "^0.5.17",
    "babel-plugin-styled-components": "^2.1.4",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.2",
    "glob": "^11.0.0",
    "node-fetch": "^3.3.2",
    "react-refresh": "^0.14.0"
  },
  "description": "## Estructura del Proyecto (Pages Router)",
